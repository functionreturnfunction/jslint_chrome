{
  "name": "JSLint for Chrome",
  "version": "1.0",
  "description": "Implementation of Douglas Crockford's JSLint for script validation from within Google Chrome.",
  "background_page" : "content/background.html",
  "browser_action": {
    "default_title" : "Get Page Scripts",
    "default_icon": "content/icon.png"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/content.js"]
    }
  ]
}
